[options]
module.file_ext=.js
module.file_ext=.jsx
module.system.node.resolve_dirname=node_modules
module.system.node.resolve_dirname=src
suppress_comment= \\(.\\|\n\\)*\\$FlowBug


[ignore]
<PROJECT_ROOT>/coverage/.*
