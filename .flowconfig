<PROJECT_ROOT>/example/.*
<PROJECT_ROOT>/node_modules/documentation
.*/graphql/.*
<PROJECT_ROOT>/.*/gatsby-node.js
<PROJECT_ROOT>/packages/enty-docs/.*
<PROJECT_ROOT>/node_modules/immutable

[untyped]
.*/lru-memoize/.*
