{
  "source": {
    "include": [ "README.md", "src" ],
    "includePattern": ".+jsx?"
  },
  "opts": {
     "destination": "./docs/",
     "recurse": true,
     "template": "node_modules/jsdonk",
     "jsdonk": {
       "title": "Fronads",
       "repo": {
         "github": "https://github.com/blueflag/fronads"
       },
       "sections": {
         "module": {
           "typedef": false
         },
         "typedef": false
       },
       "output": {
         "taffyjson": true,
         "heirarchyjson": true
       },
       "functions": {
         "multiline": false
       },
       "doclet": {
         "params": true,
         "returns": true
       },
       "debug": false
     }
  },
  "plugins": [
     "plugins/markdown",
     "node_modules/jsdoc-babel",
     "node_modules/jsdonk/lib/plugins/react-tags",
     "node_modules/jsdoc-react-proptypes/lib/plugin"
  ],
  "babel": {
    "extensions": ["js", "es6", "jsx"],
    "presets": ["blueflag"]
  }
}
