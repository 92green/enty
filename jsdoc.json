{
  "source": {
    "include": [ "README.md", "src" ],
    "includePattern": ".+jsx?"
  },
  "opts": {
     "destination": "./docs/",
     "recurse": true,
     "template": "node_modules/jsdonk",
     "jsdonk": {
        "title": "Enty API Docs",
        "sections": {
          "module": {
            "typedef": false
          },
          "typedef": false
        },
        "output": {
          "taffyjson": true,
          "heirarchyjson": true
        },
        "debug": true
     }
  },
  "plugins": [
     "plugins/markdown",
     "node_modules/jsdoc-babel",
     "node_modules/jsdonk/lib/plugins/react-tags",
     "node_modules/jsdoc-react-proptypes/lib/plugin"
  ],
  "babel": {
    "extensions": ["js", "es6", "jsx"],
    "presets": ["blueflag"]
  }
}
