{
  "name": "enty",
  "version": "0.38.1",
  "description": "An opinionated set of redux tools to cut down on boilerplate",
  "main": "lib/index.js",
  "license": "UNLICENSED",
  "author": "Allan Hortle",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blueflag/enty.git"
  },
  "files": [
    "lib"
  ],
  "bugs": {
    "url": "https://github.com/blueflag/enty/issues"
  },
  "private": false,
  "scripts": {
    "build": "rm -rf lib && babel src --out-dir lib",
    "build-examples": "cd example && yarn install && yarn run build && cd -",
    "deploy-to-gh-pages": "yarn run build && yarn run build-examples && yarn run docs && node scripts/publish.js",
    "prepublish": "yarn run build",
    "ava": "NODE_ENV=test ava",
    "check-coverage": "NODE_ENV=test nyc check-coverage --branches 83.0  --functions 83.0 --lines 83.0",
    "docs": "jsdoc -c jsdoc.json --readme README.md",
    "flow": "flow check",
    "lint": "eslint src --ext js,jsx",
    "lint-fix": "eslint src --ext js,jsx --fix",
    "test": "NODE_ENV=test nyc --report-dir=./coverage ava && yarn run check-coverage",
    "test-all": "NODE_ENV=test yarn lint && yarn flow && yarn test"
  },
  "peerDependencies": {
    "redux": "^3.6.0"
  },
  "dependencies": {
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-react": "^7.4.0",
    "fronads": "^0.12.0",
    "immutable": "^3.8.1",
    "jsdoc-react-proptypes": "^1.0.1",
    "react-redux": "^5.0.3",
    "redux-actions": "^2.0.1",
    "redux-thunk": "^2.1.0",
    "stampy": "^0.26.1"
  },
  "devDependencies": {
    "app-module-path": "^2.2.0",
    "ava": "^0.18.2",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.0",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-preset-blueflag": "^0.3.0",
    "babel-register": "^6.23.0",
    "dotenv": "^4.0.0",
    "enzyme": "^2.6.0",
    "eslint": "^3.9.1",
    "eslint-config-blueflag": "0.7.0",
    "flow-bin": "0.54",
    "gh-pages": "^0.11.0",
    "git-url-parse": "^6.0.1",
    "jsdoc": "^3.4.3",
    "jsdoc-babel": "^0.2.1",
    "jsdonk": "^0.7.0",
    "nyc": "^10.3.2",
    "proto-babel-plugin-extract-flow-types": "git+ssh://git@github.com/blueflag/proto-babel-plugin-extract-flow-types.git#v0.1.1",
    "proxyquire": "^1.7.10",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "redux": "^3.6.0",
    "require.async": "^0.1.1",
    "sinon": "^1.17.6"
  },
  "ava": {
    "require": [
      "./pretest",
      "babel-register"
    ],
    "babel": {
      "presets": [
        "blueflag"
      ],
      "plugins": [
        "istanbul"
      ],
      "sourceMaps": "inline"
    },
    "files": [
      "src/**/*-test.js"
    ]
  },
  "nyc": {
    "all": true,
    "sourceMap": false,
    "instrument": false,
    "include": [
      "src/**"
    ],
    "exclude": [
      "src/**/*-test.js",
      "example"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "extension": [
      ".js",
      ".jsx"
    ],
    "require": [
      "babel-register"
    ]
  }
}
