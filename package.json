{
  "name": "redux-blueflag",
  "version": "0.13.0",
  "description": "An opinionated set of redux tools to cut down on boilerplate",
  "main": "lib/index.js",
  "scripts": {
    "test": "yarn run lint && nyc ava",
    "ava": "ava",
    "build": "rm -rf lib; BABEL_ENV=build babel src -d lib",
    "watch": "BABEL_ENV=build babel src -d lib --watch",
    "lint": "eslint src/**/*.jsx src/**/*.js",
    "lint-fix": "eslint src/**/*.jsx src/**/*.js --fix",
    "example": "NODE_ENV=development node example/index",
    "docs": "jsdoc2md -t src/readme.hbs lib/**/*.js > README.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blueflag/redux-blueflag.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/blueflag/redux-blueflag/issues"
  },
  "homepage": "https://github.com/blueflag/redux-blueflag#readme",
  "dependencies": {
    "denormalizr": "^0.3.2",
    "immutable": "^3.8.1",
    "normalizr": "^2.2.1",
    "react-redux": "^4.4.5",
    "redux-actions": "^0.11.0",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-blueflag": "0.0.1",
    "babel-register": "^6.14.0",
    "eslint": "^3.9.1",
    "eslint-config-blueflag": "^0.2.0",
    "express": "^4.14.0",
    "express-graphql": "^0.5.4",
    "graphql": "^0.7.0",
    "jsdoc-to-markdown": "^1.3.7",
    "nyc": "^6.6.1",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-transform-hmr": "^1.0.4",
    "redux": "^3.6.0",
    "sinon": "^1.17.6",
    "superagent": "^2.2.0",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.7.0",
    "webpack-hot-middleware": "^2.12.2"
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "all": true,
    "require": [
      "babel-register"
    ],
    "include": [
      "src/*.js"
    ],
    "reporter": [
      "html",
      "text"
    ]
  }
}
