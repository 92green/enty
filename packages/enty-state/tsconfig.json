{
  "compilerOptions": {
    "outDir": "./lib/",
    "noImplicitAny": false,
    "sourceMap": true,
    "module": "commonjs",
    "target": "es5",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "typeRoots": [ "../../types", "./node_modules/@types"],
    "allowJs": false
  },
  "exclude": ["node_modules", "typings"],
	"include": ["src"]
}
