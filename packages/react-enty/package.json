{
  "name": "react-enty",
  "version": "2.0.0-alpha.2",
  "description": "React bindings for Enty",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/react-enty.esm.js",
  "license": "MIT",
  "author": "Allan Hortle",
  "size-limit": [
    {
      "path": "dist/react-enty.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react-enty.esm.js",
      "limit": "10 KB"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/92green/enty.git"
  },
  "files": [
    "dist"
  ],
  "bugs": {
    "url": "https://github.com/92green/enty/issues"
  },
  "private": false,
  "scripts": {
    "build": "yarn tsdx build",
    "lint": "yarn tsdx lint src",
    "prepublishOnly": "yarn build",
    "test": "yarn jest",
    "test-all": "yarn test && yarn build",
    "watch": "yarn tsdx watch",
    "size": "size-limit"
  },
  "peerDependencies": {
    "react": "^16.x.x"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.11.0",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/react-hooks": "^6.0.0",
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.6.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.1",
    "enzyme": "^3.11.0",
    "jest": "^26.6.3",
    "jest-enzyme": "^7.1.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "size-limit": "^4.11.0",
    "ts-jest": "^26.5.6",
    "tsdx": "^0.14.1",
    "tslib": "^2.2.0",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "enty": "^2.0.0-alpha.2"
  }
}
